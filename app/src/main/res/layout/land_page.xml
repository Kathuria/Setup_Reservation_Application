<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/user" >

    <ImageButton
        android:id="@+id/freebutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/buttonl4"
        android:layout_alignTop="@+id/buttonl5"
        android:background="@null"
        android:src="@drawable/free"
        tools:ignore="ContentDescription" />

    <ImageButton
        android:id="@+id/buttonl1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/toggleButton1"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="23dp"
        android:background="@null"
        android:src="@drawable/setup_list"
        tools:ignore="ContentDescription" />

    <ImageButton
        android:id="@+id/buttonl4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/buttonl3"
        android:layout_marginRight="33dp"
        android:background="@null"
        android:src="@drawable/cancel_setup"
        tools:ignore="ContentDescription" />

    <ImageButton
        android:id="@+id/buttonl2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/buttonl3"
        android:layout_alignLeft="@+id/buttonl4"
        android:background="@null"
        android:src="@drawable/book_setup"
        tools:ignore="ContentDescription" />

    <ImageButton
        android:id="@+id/buttonl6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/imageButtonl1"
        android:layout_marginRight="19dp"
        android:background="@null"
        android:src="@drawable/helpdesk"
        tools:ignore="ContentDescription" />

    <ToggleButton
        android:id="@+id/toggleButton1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignTop="@+id/imageButtonl1"
                android:textOn=""
        android:textOff=""
        android:layout_marginLeft="16dp"
        android:background="@null"
        android:button="@layout/check" />

    <ImageButton
        android:id="@+id/buttonl5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/buttonl3"
        android:layout_below="@+id/buttonl3"
        android:layout_marginTop="15dp"
        android:background="@null"
        android:src="@drawable/defective"
        tools:ignore="ContentDescription" />

    <ImageButton
        android:id="@+id/buttonl3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/buttonl1"
        android:layout_below="@+id/buttonl1"
        android:layout_marginTop="18dp"
        android:background="@null"
        android:src="@drawable/edit_book"
        tools:ignore="ContentDescription" />

    <ImageButton
        android:id="@+id/imageButtonl1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="14dp"
        android:layout_toRightOf="@+id/buttonl3"
        android:background="@null"
        android:src="@drawable/adduser"
        tools:ignore="ContentDescription" />

</RelativeLayout>