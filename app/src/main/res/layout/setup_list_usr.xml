<?xml version="1.0" encoding="utf-8"?>


<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:scrollbars="vertical"
    android:background="@drawable/user" >

   <TextView
       android:id="@+id/textViewn1"
       android:layout_width="fill_parent"
       android:layout_height="wrap_content"
       android:layout_marginTop="14dp"
       android:text="Setup Details"
       android:textAlignment="center"
       android:textColor="#EF871F"
       android:textSize="30sp"
       tools:ignore="HardcodedText" />

   <Button
       android:id="@+id/availbuttonU"
       android:layout_width="match_parent"
       android:layout_height="wrap_content"
       android:layout_alignLeft="@+id/textViewn1"
       android:layout_below="@+id/textViewn1"
       android:layout_marginTop="36dp"
       android:text="Available Setups"
       android:background="#EF871F"
       android:textColor="#FFFFFF"
       tools:ignore="HardcodedText" />

   <Button
       android:id="@+id/bookedbuttonU"
       android:layout_width="match_parent"
       android:layout_height="wrap_content"
       android:layout_alignLeft="@+id/availbuttonU"
       android:layout_below="@+id/availbuttonU"
       android:layout_marginTop="46dp"
       android:text="Booked Setups"
       android:background="#EF871F"
       android:textColor="#FFFFFF"
       tools:ignore="HardcodedText" />

   <Button
       android:id="@+id/allbuttonU"
       android:layout_width="match_parent"
       android:layout_height="wrap_content"
       android:layout_alignLeft="@+id/bookedbuttonU"
       android:layout_below="@+id/bookedbuttonU"
       android:layout_marginTop="46dp"
       android:text="All Setups"
       android:background="#EF871F"
       android:textColor="#FFFFFF"
       tools:ignore="HardcodedText" />

</RelativeLayout>
